<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk" version="4.0"/>
    <requires lib="adw" version="1.0"/>
    <template class="ExampleWindow" parent="GtkApplicationWindow">
        <property name="title">Python e GTK 4: PyGObject Gtk.TreeView() filter ui file</property>
        <property name="default-width">683</property>
        <property name="default-height">384</property>
        <child type="titlebar">
            <object class="GtkHeaderBar" id="header_bar">
                <child type="end">
                    <object class="GtkMenuButton">
                        <property name="icon-name">open-menu-symbolic</property>
                        <property name="menu-model">primary_menu</property>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkBox">
                <property name="orientation">vertical</property>
                <property name="margin-top">12</property>
                <property name="margin-end">12</property>
                <property name="margin-bottom">12</property>
                <property name="margin-start">12</property>
                <property name="spacing">12</property>
                <!-- GtkTreeView -->
                <child>
                    <object class="GtkScrolledWindow">
                        <property name="vexpand">true</property>
                        <child>
                            <object class="GtkTreeView" id="treeview">
                                <property name="model">model</property>
                                <child>
                                    <object class="GtkTreeViewColumn">
                                        <property name="title">Software</property>
                                        <child>
                                            <object class="GtkCellRendererText"/>
                                            <attributes>
                                                <attribute name="text">0</attribute>
                                            </attributes>
                                        </child>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkTreeViewColumn">
                                        <property name="title">Lan√ßamento</property>
                                        <child>
                                            <object class="GtkCellRendererText"/>
                                            <attributes>
                                                <attribute name="text">1</attribute>
                                            </attributes>
                                        </child>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkTreeViewColumn">
                                        <property name="title">Linguagem</property>
                                        <child>
                                            <object class="GtkCellRendererText"/>
                                            <attributes>
                                                <attribute name="text">2</attribute>
                                            </attributes>
                                        </child>
                                    </object>
                                </child>
                                <child internal-child="selection">
                                    <object class="GtkTreeSelection" id="selection">
                                    </object>
                                </child>
                            </object>
                        </child>
                        <!-- End GtkTreeView -->
                    </object>
                </child>
                <child>
                    <object class="GtkBox">
                        <property name="orientation">horizontal</property>
                        <property name="homogeneous">true</property>
                        <property name="margin-top">12</property>
                        <property name="margin-end">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="margin-start">12</property>
                        <property name="spacing">12</property>
                        <child>
                            <object class="GtkButton">
                                <property name="label">Java</property>
                                <signal name="clicked" handler="on_button_clicked" swapped="no"/>
                            </object>
                        </child>
                        <child>
                            <object class="GtkButton">
                                <property name="label">C</property>
                                <signal name="clicked" handler="on_button_clicked" swapped="no"/>
                            </object>
                        </child>
                        <child>
                            <object class="GtkButton">
                                <property name="label">C++</property>
                                <signal name="clicked" handler="on_button_clicked" swapped="no"/>
                            </object>
                        </child>
                        <child>
                            <object class="GtkButton">
                                <property name="label">Python</property>
                                <signal name="clicked" handler="on_button_clicked" swapped="no"/>
                            </object>
                        </child>
                        <child>
                            <object class="GtkButton">
                                <property name="label">todos</property>
                                <signal name="clicked" handler="on_button_clicked" swapped="no"/>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>

    <object class="GtkListStore" id="model">
        <columns>
            <column type="gchararray"/>
            <column type="gchararray"/>
            <column type="gchararray"/>
        </columns>
        <data>
            <row>
                <col id="0">Firefox</col>
                <col id="1">2002</col>
                <col id="2">C++</col>
            </row>
