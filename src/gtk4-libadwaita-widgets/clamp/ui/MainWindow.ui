<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk" version="4.0"/>
    <requires lib="adw" version="1.0"/>
    <template class="ExampleWindow" parent="GtkApplicationWindow">
        <property name="title">Python e GTK 4: PyGObject libadwaita Adw.Clamp() ui file</property>
        <property name="default-width">683</property>
        <property name="default-height">384</property>
        <child type="titlebar">
            <object class="GtkHeaderBar" id="header_bar">
                <child type="end">
                    <object class="GtkMenuButton">
                        <property name="icon-name">open-menu-symbolic</property>
                        <property name="menu-model">primary_menu</property>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="AdwClamp">
                <property name="child">
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="margin-top">24</property>
                        <property name="margin-bottom">24</property>
                        <property name="margin-start">12</property>
                        <property name="margin-end">12</property>
                        <property name="spacing">24</property>
                        <child>
                            <object class="GtkListBox">
                                <property name="selection-mode">none</property>
                                <style>
                                    <class name="boxed-list"/>
                                </style>
                                <child>
                                    <object class="GtkListBoxRow">
                                        <child>
                                            <object class="GtkLabel">
                                                <property name="label">Item 01</property>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkListBoxRow">
                                        <child>
                                            <object class="GtkLabel">
                                                <property name="label">Item 02</property>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkListBoxRow">
                                        <child>
                                            <object class="GtkLabel">
                                                <property name="label">Item 03</property>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </child>
                        <child>
                            <object class="GtkListBox">
                                <property name="selection-mode">none</property>
                                <style>
                                    <class name="boxed-list"/>
                                </style>

                                <child>
                                    <object class="GtkListBoxRow">
                                        <child>
                                            <object class="GtkLabel">
                                                <property name="label">Item 04</property>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkListBoxRow">
                                        <child>
                                            <object class="GtkLabel">
                                                <property name="label">Item 05</property>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkListBoxRow">
                                        <child>
                                            <object class="GtkLabel">
                                                <property name="label">Item 06</property>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </property>
            </object>
        </child>
    </template>

    <menu id="primary_menu">
        <section>
            <item>
                <attribute name="label" translatable="yes">_Preferences</attribute>
                <attribute name="action">app.preferences</attribute>
            </item>
        </section>
    </menu>
</interface>